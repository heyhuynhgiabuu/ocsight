{
  "name": "ocsight",
  "version": "0.7.4",
  "description": "OpenCode ecosystem observability platform",
  "main": "dist/lib/index.js",
  "bin": {
    "ocsight": "dist/lib/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "bundle": "node esbuild.config.cjs",
    "prepack": "bun run build && bun run bundle && mkdir -p dist/lib && cp dist-bundle/index.js dist/lib/index.js",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "release:patch": "bun run script/publish.ts",
    "release:minor": "bun run script/publish.ts",
    "release:major": "bun run script/publish.ts"
  },
  "keywords": [
    "opencode",
    "observability",
    "analytics",
    "cli",
    "mcp"
  ],
  "author": "OpenCode",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "bun-types": "latest",
    "esbuild": "^0.19.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ]
}
