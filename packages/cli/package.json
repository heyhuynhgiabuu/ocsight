{
  "name": "@heyhuynhgiabuu/ocsight-cli",
  "version": "1.1.4",
  "description": "OpenCode ecosystem observability CLI",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "bun": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "bundle-cli": "bun run ../../scripts/bundle-cli.ts",
    "dev": "bun run src/index.ts",
    "test": "bun test --coverage",
    "test:concurrent": "bun test --concurrent",
    "test:watch": "bun test --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "opencode",
    "observability",
    "analytics",
    "cli"
  ],
  "author": "OpenCode",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/heyhuynhgiabuu/ocsight.git"
  },
  "bugs": {
    "url": "https://github.com/heyhuynhgiabuu/ocsight/issues"
  },
  "homepage": "https://github.com/heyhuynhgiabuu/ocsight#readme",
  "dependencies": {
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "csv-writer": "^1.6.0",
    "glob": "^10.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "bun-types": "latest",
    "typescript": "^5.7.2"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
