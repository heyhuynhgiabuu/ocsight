{
  "name": "ocsight-website",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro build && astro preview",
    "build": "astro build",
    "preview": "astro preview",
    "docs:dev": "astro dev",
    "docs:build": "astro build",
    "docs:preview": "astro preview",
    "docs:check": "astro check",
    "deploy:dev": "astro build && wrangler pages deploy dist --project-name=ocsight-website --branch=dev",
    "deploy:prod": "astro build && wrangler pages deploy dist --project-name=ocsight-website --branch=production",
    "validate": "bun run scripts/validate-content.ts",
    "check-links": "bun run scripts/check-links.ts",
    "content:validate": "bun run validate && bun run check-links",
    "content:build": "bun run content:validate && bun run build"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.6.8",
    "@astrojs/markdown-remark": "^6.3.6",
    "@astrojs/mdx": "^4.0.0",
    "@astrojs/node": "^9.0.0",
    "@astrojs/react": "^4.0.0",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/starlight": "^0.35.3",
    "@astrojs/tailwind": "^6.0.2",
    "@fontsource/ibm-plex-mono": "^5.2.7",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "astro": "^5.0.0",
    "astro-compress": "^2.3.0",
    "astro-htmx": "^1.0.0",
    "clsx": "^2.0.0",
    "expressive-code": "^0.41.3",
    "htmx.org": "^2.0.0",
    "jiti": "^2.5.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.0.0",
    "rehype-autolink-headings": "^7.1.0",
    "sharp": "^0.33.0",
    "shiki": "^3.13.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.14.0"
  }
}
